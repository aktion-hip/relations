      </initializer>
   </extension>
   <extension
         point="org.eclipse.core.expressions.definitions">
      <definition
            id="org.elbe.relations.definition.browser.popup.item">
         <with
               variable="browser.popup.visibility">
            <or>
	            <equals
	                  value="item_center">
	            </equals>
               <equals
                     value="item_periphery">
               </equals>
               </or>
         </with>
      </definition>
      <definition
            id="org.elbe.relations.definition.browser.popup.connection">
         <with
               variable="browser.popup.visibility">
            <equals
                  value="connection">
            </equals>
         </with>
      </definition>
   </extension>
